<link rel="stylesheet" href="{{ 'stainless-steel-pan.css' | asset_url }}" media="print" onload="this.media='all'" />
<noscript>{{ 'stainless-steel-pan.css' | asset_url | stylesheet_tag }}</noscript>

<div class="stainless-steel-pan section-padding">
  <div class="image-wrapper">
    {% if section.settings.videowise != blank  %}
      <div class="image"> 
     {{ section.settings.videowise }}
      </div>
    {% else %}
    {% if section.settings.desktop_image != blank  %}
      <div class="image"> 
      {%- liquid                
        assign sizes = '(min-width: 1441px) 50.52083vw, (min-width: 1301px) 63.8889vw, (min-width: 750px) 100vw, 76.9231vw'
        assign widths = '375, 550, 750, 1100, 1500, 1780, 2000, 3000, 3840'
      -%} 
        {%- if section.settings.desktop_image != blank -%}            
          {%- liquid
            assign height = section.settings.desktop_image.width | divided_by: section.settings.desktop_image.aspect_ratio | round
          -%} 
          {% assign dek_class = '' %}
          {%- if section.settings.mobile_image != blank -%}
            {% assign dek_class = 'desktop-img' %}
          {%- endif -%}
          {{
            section.settings.desktop_image
            | image_url: width: 3840
            | image_tag: loading: 'lazy', width: section.settings.desktop_image.width, height: height, sizes: sizes, widths: widths, class: dek_class
          }}
        {%- endif -%}
        {%- if section.settings.mobile_image != blank -%}
          {{
            section.settings.mobile_image
            | image_url: width: 750
            | image_tag: loading: 'lazy', width: section.settings.mobile_image.width, height: section.settings.mobile_image.height, sizes: sizes, widths: widths, class: 'mobile-img'
          }}
        {%- endif -%}{% comment %}
        <img src="{{ section.settings.desktop_image | img_url: 'master' }}" alt="{{ section.settings.image_alt_text }}" class="desktop-img">
        <img src="{{ section.settings.mobile_image | img_url: 'master' }}" alt="{{ section.settings.image_alt_text }}" class="mobile-img">{% endcomment %}
      </div>
    {% endif %}
    {% endif %}
     {% if section.settings.subtitle_text != blank  %}
    <div class="subtitle">{{ section.settings.subtitle_text }}</div>
    {% endif %}
    <div class="pan_review review1">
      <div class="reviews">
        {% if section.settings.review_image != blank %}
          <div class="reviews"><img src="{{ section.settings.review_image | image_url: width: 128 }}" alt="" width="128" height="24" loading="lazy" /></div>
        {% endif %}
      </div>
      <div class="auther-message"> <h2 class="heading h2">{{ section.settings.review_message }}</h2> </div>
      <div class="auther-details"> 
        <div class="auther-image">
          {% if section.settings.author_image != blank %}
            <div class="auther-image">
              <img src="{{ section.settings.author_image | image_url: width: 100 }}" alt="" width="80" height="80" loading="lazy" />              
            </div>
          {% endif %}
        </div> 
         {% if section.settings.author_name != blank %}
          <div class="auther-wrapper"> 
            <div class="auther-name">{{ section.settings.author_name }}</div>
            <div class="auther-verification">  
              <span class="check-icon">  
                <svg width="12" height="12" viewBox="0 0 12 12" fill="none" xmlns="http://www.w3.org/2000/svg">  
                  <rect width="12" height="12" rx="6" fill="#A6BB3A"></rect>  
                  <path d="M3.375 6.00984L5.11734 7.80469L8.625 4.19531" stroke="#FCFBF7" stroke-width="1.5" stroke-linecap="round" stroke-linejoin="round"></path>  
                </svg>  
              </span>  
             <span>{{ section.settings.verification_text }}</span>
            </div> 
          </div> 
          {% endif %}
      </div>
    </div>
    <div class="pan_review review2">
     {% if section.settings.custom_heading_text != blank %}
      <div class="auther-message"> <h2 class="heading h2">{{ section.settings.custom_heading_text }}</h2> </div>
      {% endif %}
      <div class="auther-details"> 
        {% if section.settings.author_image_2 != blank %}
        <div class="author-image">
          <img src="{{ section.settings.author_image_2 | image_url: width: 100 }}" alt="" width="80" height="80" loading="lazy" />
        </div>
       {% endif %}
       {% if section.settings.author_name_2 != blank %}
        <div class="auther-wrapper"> 
          <div class="author-name">{{ section.settings.author_name_2 }}</div>
            <div class="auther-verification">  
              <span class="check-icon">  
                <svg width="12" height="12" viewBox="0 0 12 12" fill="none" xmlns="http://www.w3.org/2000/svg">
                  <rect width="12" height="12" rx="6" fill="#606D22"/>
                  <path d="M3.375 6.00984L5.11734 7.80469L8.625 4.19531" stroke="#FCFBF7" stroke-width="1.5" stroke-linecap="round" stroke-linejoin="round"/>
                </svg> 
              </span>  
              <span>{{ section.settings.verification_text_2 }}</span>
            </div> 
        </div> 
        {% endif %}         
      </div>
    </div>
